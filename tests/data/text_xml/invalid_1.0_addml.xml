<?xml version="1.0" encoding="UTF-8"?>
<addml:addml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.arkivverket.no/standarder/addml local_8.3_addml.xsd"
             xmlns:addml="http://www.arkivverket.no/standarder/addml">
    <addml:dataset name="Testdata">
        <addml:descriptionss>Testdata</addml:descriptionss>
        <addml:reference/>
        <addml:flatFiles>
            <addml:flatFile name="csvfile.csv" definitionReference="testdef">
            </addml:flatFile>
            <addml:flatFileDefinitions>
                <addml:flatFileDefinition name="testdef"
                                          typeReference="testtype">
                    <addml:recordDefinitions>
                        <addml:recordDefinition name="testrecord"
                                                typeReference="testrectype">
                            <addml:fieldDefinitions>
                                <addml:fieldDefinition name="test"
                                                       typeReference="String">
                                    <addml:description/>
                                </addml:fieldDefinition>
                                <addml:fieldDefinition name="test"
                                                       typeReference="String">
                                    <addml:description/>
                                </addml:fieldDefinition>
                                <addml:fieldDefinition name="test"
                                                       typeReference="String">
                                    <addml:description/>
                                </addml:fieldDefinition>
                            </addml:fieldDefinitions>
                        </addml:recordDefinition>
                    </addml:recordDefinitions>
                </addml:flatFileDefinition>
            </addml:flatFileDefinitions>
            <addml:structureTypes>
                <addml:flatFileTypes>
                    <addml:flatFileType name="testtype">
                        <addml:charset>UTF-8</addml:charset>
                        <addml:delimFileFormat>
                            <addml:recordSeparator>CR+LF
                            </addml:recordSeparator>
                            <addml:fieldSeparatingChar>;
                            </addml:fieldSeparatingChar>
                            <addml:quotingChar>"</addml:quotingChar>
                        </addml:delimFileFormat>
                    </addml:flatFileType>
                </addml:flatFileTypes>
                <addml:recordTypes>
                    <addml:recordType name="testrectype">
                        <addml:trimmed/>
                    </addml:recordType>
                </addml:recordTypes>
                <addml:fieldTypes>
                    <addml:fieldType name="String">
                        <addml:dataType>string</addml:dataType>
                    </addml:fieldType>
                </addml:fieldTypes>
            </addml:structureTypes>
        </addml:flatFiles>
    </addml:dataset>
</addml:addml>
